    "nx:affected:build": "nx affected --target=build",
    "nx:affected:test": "nx affected --target=test",
    "nx:affected:lint": "nx affected --target=lint",
    "nx:dep-graph": "nx dep-graph"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "^18.0.0",
    "@nx/jest": "^18.0.0",
    "@nx/js": "^18.0.0",
    "@nx/nest": "^18.0.0",
    "@nx/node": "^18.0.0",
    "@nx/webpack": "^18.0.0",
    "@nx/workspace": "^18.0.0",
    "@types/express-session": "^1.18.2",
    "nx": "^18.0.0",
