    "test:e2e": "cd apps/api && npm run test:e2e",
    "lint": "cd apps/api && npm run lint",
    "format": "cd apps/api && npm run format",
    "migration:run": "cd apps/api && npm run migration:run",
    "migration:generate": "cd apps/api && npm run migration:generate",
    "migration:revert": "cd apps/api && npm run migration:revert",
    "nx:serve": "nx serve api",
    "nx:build": "nx build api",
