{
  "name": "b2b-ecommerce-api",
  "version": "1.0.0",
  "description": "B2B E-Commerce Platform API with Clean Architecture, DDD, and CQRS",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/typeorm.config.ts",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed-database.ts",
    "seed:orders": "ts-node -r tsconfig-paths/register src/scripts/seed-orders.ts",
    "seed:keycloak": "ts-node -r tsconfig-paths/register src/scripts/seed-keycloak-users.ts",
    "generate:receipts": "ts-node -r tsconfig-paths/register src/scripts/generate-receipts.ts",
    "update-orders-user-id": "ts-node -r tsconfig-paths/register src/scripts/update-orders-user-id.ts",
    "fix-orders-country-codes": "ts-node -r tsconfig-paths/register src/scripts/fix-orders-country-codes.ts",
    "check-orders-user-ids": "ts-node -r tsconfig-paths/register src/scripts/check-orders-user-ids.ts",
    "map-user-ids": "ts-node -r tsconfig-paths/register src/scripts/map-user-ids-to-emails.ts",
    "keycloak:get-key": "node scripts/get-keycloak-public-key.js",
    "db:setup": "node scripts/run-setup.js",
    "db:check": "node scripts/run-check.js",
    "db:test": "node scripts/test-connection.js",
    "db:connect": "docker exec -it b2b-ecommerce-postgres psql -U ecommerce -d b2b_ecommerce",
    "db:reset": "node scripts/run-reset.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:restart": "docker-compose restart postgres"
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^23.0.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/typeorm": "^10.0.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.3",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.2",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "keycloak-connect": "^23.0.3",
    "minio": "^7.1.4",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/amqplib": "^0.10.4",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.0",
    "@types/pdfkit": "^0.13.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.17",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.2.4",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
